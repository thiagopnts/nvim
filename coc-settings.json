{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "snippets.ultisnips.directories": ["UltiSnips", "gosnippets/UltiSnips"],
  "python.formatting.provider": "black",
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "markdown",
    "javascript",
    "typescript",
    "json",
    "jsonc"
  ]
}
